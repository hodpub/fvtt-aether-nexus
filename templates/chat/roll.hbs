<div class="aether-nexus">
  <div class="dice-roll">
    {{#if flavor}}
    <div class="dice-flavor">{{{flavor}}}</div>
    {{/if}}
    <div class="dice-result">
      {{{tooltip}}}
      <h4 class="dice-total {{cssClass}}">
        {{!-- {{#if roll.countSuccess}} --}}
        {{#if rollResult}}
        {{rollResult}}: {{total}}
        {{else}}
        {{total}}
        {{/if}}
      </h4>
      {{#if additional}}
      {{{additional}}}
      {{/if}}
    </div>
  </div>
</div>